<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\FlightInformationRunOutRotor" table="flight_information_run_out_rotor">

        <id name="primaryKey" type="integer">
            <generator/>
        </id>

        <field name="rndLeftRaw" type="decimal" scale="1" precision="5" column="rnd_left_raw"/>
        <field name="rndRightRaw" type="decimal" scale="1" precision="5" column="rnd_right_raw"/>
        <field name="rvdLeftRaw" type="decimal" scale="1" precision="5" column="rvd_left_raw"/>
        <field name="rvdRightRaw" type="decimal" scale="1" precision="5" column="rvd_right_raw"/>
        <field name="rndLeftCalc" type="decimal" scale="1" precision="5" column="rnd_left_calc"/>
        <field name="rndRightCalc" type="decimal" scale="1" precision="5" column="rnd_right_calc"/>
        <field name="rvdLeftCalc" type="decimal" scale="1" precision="5" column="rvd_left_calc"/>
        <field name="rvdRightCalc" type="decimal" scale="1" precision="5" column="rvd_right_calc"/>
    </entity>

</doctrine-mapping>