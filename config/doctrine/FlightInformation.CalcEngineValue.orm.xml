<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <embeddable name="App\Entity\FlightInformation\CalcEngineValue">
        <field name="average" type="decimal" scale="3" precision="6"/>
        <field name="sampleVariance" type="decimal" scale="3" precision="8"/>
        <field name="rootMeanSquareDeviation" type="decimal" scale="3" precision="6"/>
        <field name="coefficientOfVariation" type="decimal" scale="3" precision="6"/>
        <field name="standardErrorOfTheMean" type="decimal" scale="3" precision="6"/>
        <field name="numberOfDegreesOfFreedom" type="decimal" scale="3" precision="6"/>
    </embeddable>
</doctrine-mapping>